<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
	
	<servlet>
        <servlet-name>Uploader</servlet-name>
        <servlet-class>servlet.Uploader</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Uploader</servlet-name>
        <url-pattern>/uploader</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Downloader</servlet-name>
        <servlet-class>servlet.Downloader</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Downloader</servlet-name>
        <url-pattern>/downloader</url-pattern>
    </servlet-mapping>
    
</web-app>
